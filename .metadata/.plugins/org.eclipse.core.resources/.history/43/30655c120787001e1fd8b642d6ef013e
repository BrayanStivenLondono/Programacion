package mas;

public class Mas06 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double aleatorio = Math.random()*101;
		
		// lo paso a int
		
		int enteroAleatorio = (int) aleatorio;
		
		for (int i= enteroAleatorio; i>=-100; i=i+7) {
			System.out.println(i);
		}

	}

}
